<%= simple_form_for(@tipo_ocorrencia) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :descricao %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class=>'btn btn-primary' %>
    <%= link_to 'Voltar', tipo_ocorrencias_path, :class=>'btn btn-default' %>
  </div>
<% end %>
